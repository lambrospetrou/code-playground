{
  "name": "minimal-shadow-cljs-nodejs",
  "version": "0.1.0",
  "description": "ClojureScript script compiled to Node",
  "scripts": {
    "release": "run-s release:*",
    "release:app": "shadow-cljs release app",
    "watch:app": "shadow-cljs watch app",
    "compile:app": "shadow-cljs compile app",
    "release:library": "shadow-cljs release library",
    "watch:library": "shadow-cljs watch library",
    "compile:library": "shadow-cljs compile library",
    "release:browser": "shadow-cljs release browser",
    "watch:browser": "shadow-cljs watch browser",
    "compile:browser": "shadow-cljs compile browser",
    "start:app": "node build/node-app.js",
    "start:browser": "serve -s build/browser"
  },
  "keywords": [
    "shadow-cljs",
    "clojurescript",
    "node",
    "browser",
    "commonjs",
    "clojure"
  ],
  "author": "lambrospetrou",
  "license": "MIT",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "serve": "^11.1.0",
    "shadow-cljs": "^2.8.39",
    "source-map-support": "^0.5.12",
    "ws": "^7.4.6"
  }
}
